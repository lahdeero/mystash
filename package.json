{
  "name": "mystash",
  "private": true,
  "workspaces": ["packages/*"],
  "scripts": {
    "init": "npm install",
    "reset:deps": "npm run remove:deps && npm run init",
    "remove:deps": "find . -type d -name 'node_modules' -prune -exec rm -rf '{}' +"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.8.1",
    "typescript": "^4.9.5",
    "axios": "^1.7.7"
  }
}
